{
  "name": "fewa-universal-app",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "prebuild": "expo prebuild",
    "test": "jest --watchAll",
    "lint": "expo lint",
    "db:migrate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "build:android": "eas build --platform android --local",
    "build:ios": "eas build --platform ios --local",
    "build:web": "expo export -p web"
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "@oslojs/encoding",
          "react-redux",
          "@trpc/client",
          "@trpc/react-query",
          "@trpc/server",
          "drizzle-orm",
          "eslint-plugin-react-compiler",
          "postgres",
          "superjson"
        ]
      }
    }
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.2",
    "@oslojs/encoding": "1.1.0",
    "@react-navigation/bottom-tabs": "^7.0.0",
    "@react-navigation/native": "^7.0.0",
    "@tanstack/react-query": "5.61.3",
    "@trpc/client": "11.0.0-rc.643",
    "@trpc/react-query": "11.0.0-rc.643",
    "@trpc/server": "11.0.0-rc.643",
    "babel-plugin-react-compiler": "^19.0.0-beta-df7b47d-20241124",
    "drizzle-orm": "0.36.4",
    "eslint-plugin-react-compiler": "^19.0.0-beta-df7b47d-20241124",
    "expo": "~52.0.14",
    "expo-blur": "~14.0.1",
    "expo-constants": "~17.0.3",
    "expo-font": "~13.0.1",
    "expo-haptics": "~14.0.0",
    "expo-linking": "~7.0.3",
    "expo-router": "~4.0.11",
    "expo-splash-screen": "~0.29.13",
    "expo-status-bar": "~2.0.0",
    "expo-symbols": "~0.2.0",
    "expo-system-ui": "~4.0.5",
    "expo-web-browser": "~14.0.1",
    "postgres": "3.4.5",
    "react": "18.3.1",
    "react-compiler-runtime": "^19.0.0-beta-df7b47d-20241124",
    "react-dom": "18.3.1",
    "react-native": "0.76.3",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.1.0",
    "react-native-web": "~0.19.13",
    "react-native-webview": "13.12.5",
    "superjson": "2.2.1",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/jest": "^29.5.12",
    "@types/react": "~18.3.12",
    "@types/react-test-renderer": "^18.3.0",
    "drizzle-kit": "0.28.1",
    "eslint": "^8.57.0",
    "eslint-config-expo": "~8.0.1",
    "jest": "^29.2.1",
    "jest-expo": "~52.0.2",
    "react-test-renderer": "18.3.1",
    "typescript": "^5.3.3"
  },
  "private": true
}
